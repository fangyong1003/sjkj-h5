{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": [
    "transform-vue-jsx",
    "transform-runtime",
    "transform-object-rest-spread",
    ["import", [
      { "libraryName": "vant", "style": true },
      { "libraryName": "captain-ui", "style": true }
    ]]
  ]
}
