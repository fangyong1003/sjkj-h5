/* 公用样式，icon */

.icon-check {
  display: inline-block;
  float: right;
  width: 18px;
  height: 50px;
  margin-right: 5px;
  margin-top:-10px;
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: 18px;
  background-position: center center;
}

.icon-check-active {
  background-size: 18px;
  background-image: url("./images/checked.png");
}
